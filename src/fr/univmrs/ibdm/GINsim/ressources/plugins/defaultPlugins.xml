<?xml version="1.0" encoding="UTF-8"?>
<GINsim-addon>
	<listOfPlugins>
		<plugin name="simulation" mainClass="fr.univmrs.ibdm.GINsim.reg2dyn.Reg2DynPlugin" load="true"/>
		<plugin name="layout" mainClass="fr.univmrs.ibdm.GINsim.layout.GsLayoutPlugin" load="true"/>
		<plugin name="layoutJGraph" mainClass="fr.univmrs.ibdm.GINsim.jgraph.layout.GsJgraphLayout" load="false"/>
		<plugin name="circuit" mainClass="fr.univmrs.ibdm.GINsim.circuit.GsCircuitPlugin" load="true" depend=""/>
		<plugin name="modelChecker" mainClass="fr.univmrs.ibdm.GINsim.modelChecker.GsModelCheckerPlugin" load="true" depend=""/>
		<plugin name="export" mainClass="fr.univmrs.ibdm.GINsim.export.GsExportPlugin" load="true" depend=""/>
		<plugin name="connectivity" mainClass="fr.univmrs.ibdm.GINsim.connectivity.ConnectivityPlugin" load="true" depend=""/>
		<plugin name="animate" mainClass="fr.univmrs.ibdm.GINsim.aRegGraph.GsARegGraphPlugin" load="true"/>
		<plugin name="analyseStateTransition" mainClass="fr.univmrs.ibdm.GINsim.dynamicGraph.GsDynamicAnalyserPlugin" load="true"/>
	</listOfPlugins>
	
	<listOfGraphType>
		<graph name="regulatoryGraph" mainClass="fr.univmrs.ibdm.GINsim.regulatoryGraph.GsRegulatoryGraphDescriptor" load="true"/>
		<graph name="stateTransitionGraph" mainClass="fr.univmrs.ibdm.GINsim.dynamicGraph.GsDynamicGraphDescriptor" load="true"/>
		<graph name="connectedComponentGraph" mainClass="fr.univmrs.ibdm.GINsim.connectivity.GsReducedGraphDescriptor" load="true"/>
	</listOfGraphType>
	
	<imagePath path="/fr/univmrs/ibdm/GINsim/ressources/icons"/>
	
	<messagesPath path="fr.univmrs.ibdm.GINsim.ressources.messages"/>
</GINsim-addon>
