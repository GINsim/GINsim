<?xml version="1.0" encoding="UTF-8"?>
<GINsim-addon>
	<listOfPlugins>
		<!-- generic plugins -->
		<plugin name="layout"					 mainClass="fr.univmrs.tagc.GINsim.layout.GsLayoutPlugin"/>
		<plugin name="layoutmulti"				 mainClass="fr.univmrs.tagc.GINsim.layout.GsMultidimentionLayout"/>
		<plugin name="layout3D"					 mainClass="fr.univmrs.tagc.GINsim.layout.Gs3DLayout"/>
		<plugin name="layoutJGraph" load="false" mainClass="fr.univmrs.tagc.GINsim.jgraph.layout.GsJgraphLayout"/>
		<plugin name="connectivity" 			 mainClass="fr.univmrs.tagc.GINsim.connectivity.ConnectivityPlugin"/>

		<!-- analysis plugins -->
		<plugin name="simulation" 				mainClass="fr.univmrs.tagc.GINsim.reg2dyn.Reg2DynPlugin"/>
		<plugin name="circuit"					mainClass="fr.univmrs.tagc.GINsim.circuit.GsCircuitPlugin"/>
		<plugin name="stableStates" 			mainClass="fr.univmrs.tagc.GINsim.stableStates.GsStableStatesPlugin"/>
		<plugin name="analyseStateTransition"	mainClass="fr.univmrs.tagc.GINsim.dynamicGraph.GsDynamicAnalyserPlugin"/>
		<plugin name="modelSimplifier" 			mainClass="fr.univmrs.tagc.GINsim.regulatoryGraph.modelModifier.ModelSimplifierPlugin"/>
		<plugin name="interactionAnalysis" 		mainClass="fr.univmrs.tagc.GINsim.interactionAnalysis.InteractionAnalysisPlugin"/>
		<plugin name="graphComparator" 			mainClass="fr.univmrs.tagc.GINsim.graphComparator.GraphComparatorPlugin"/>
		<plugin name="stateInRegGraph" 			mainClass="fr.univmrs.tagc.GINsim.stateInRegulatoryGraph.GsStateInRegGraphPlugin"/>
		<plugin name="regFunctionsTreeView" 	mainClass="fr.univmrs.tagc.GINsim.treeViewer.GsRegulatoryFunctionTreeViewPlugin"/>
		<plugin name="pathFinding" 				mainClass="fr.univmrs.tagc.GINsim.pathFinding.PathFindingPlugin"/>
		<plugin name="stg2htg" 		load="false"			mainClass="fr.univmrs.tagc.GINsim.stg2htg.STG2HTGPlugin"/>
		<plugin name="omddViz" 		load="false"			mainClass="fr.univmrs.tagc.GINsim.omddViz.OmddVizPlugin"/>
		<plugin name="localGraph" 				mainClass="fr.univmrs.tagc.GINsim.regulatoryGraph.localGraph.LocalGraphPlugin"/>
		
		<!-- import plugins -->
		<plugin name="SBMLimport"                   mainClass="fr.univmrs.tagc.GINsim.regulatoryGraph.SBML.SBMLImportPlugin"/>

		<!-- export plugins -->
		<plugin name="export" 					mainClass="fr.univmrs.tagc.GINsim.export.GsExportPlugin"/>
		<plugin name="GenericDocumentExport"	mainClass="fr.univmrs.tagc.GINsim.export.regulatoryGraph.GenericDocumentExport"/>
		<plugin	name="CytoscapeExport" 			mainClass="fr.univmrs.tagc.GINsim.export.regulatoryGraph.CytoscapeExport"/>
		<plugin name="PNexport" 				mainClass="fr.univmrs.tagc.GINsim.export.regulatoryGraph.GsPetriNetExport"/>
		<plugin name="NuSMVexport" 				mainClass="fr.univmrs.tagc.GINsim.export.regulatoryGraph.GsNuSMVExport"/>
		<plugin name="GNAexport" 				mainClass="fr.univmrs.tagc.GINsim.export.regulatoryGraph.GsGNAExport"/>
		<plugin name="GNAMLexport" 				mainClass="fr.univmrs.tagc.GINsim.export.regulatoryGraph.GsGNAMLExport"/>
		<plugin name="SnakesExport"				mainClass="fr.univmrs.tagc.GINsim.export.regulatoryGraph.SnakesExport"/>
        <plugin name="SBMLExport"               mainClass="fr.univmrs.tagc.GINsim.export.regulatoryGraph.SBML3Export"/>

		<!-- other plugins -->
		<plugin name="animate" 					mainClass="fr.univmrs.tagc.GINsim.animator.GsARegGraphPlugin"/>
<!--
		<plugin name="tb" 						mainClass="fr.univmrs.tagc.GINsim.gui.tbclient.GsTBPlugin"/>
		<plugin name="jython" load="false"		mainClass="fr.univmrs.tagc.GINsim.jython.JythonPlugin"/>
-->
	</listOfPlugins>

	<listOfGraphType>
		<graph name="regulatoryGraph" 			mainClass="fr.univmrs.tagc.GINsim.regulatoryGraph.GsRegulatoryGraphDescriptor" load="true"/>
		<graph name="stateTransitionGraph" 		mainClass="fr.univmrs.tagc.GINsim.dynamicGraph.GsDynamicGraphDescriptor" load="true"/>
		<graph name="connectedComponentGraph" 	mainClass="fr.univmrs.tagc.GINsim.connectivity.GsReducedGraphDescriptor" load="true"/>
		<graph name="dynamicalHierarchicalGraph" 	mainClass="fr.univmrs.tagc.GINsim.dynamicalHierachicalGraph.GsDynamicalHierarchicalGraphDescriptor" load="true"/>
	</listOfGraphType>

	<imagePath 		path="/fr/univmrs/tagc/GINsim/resources/icons"/>
	<messagesPath 	path="fr.univmrs.tagc.GINsim.resources.messages"/>
	<doap 			path="/fr/univmrs/tagc/GINsim/resources/GINsim-about.rdf"/>
</GINsim-addon>
